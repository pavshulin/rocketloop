{
  "name": "authenticator",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": "v16.14.2"
  },
  "scripts": {
    "sequelize:all": "node_modules/.bin/sequelize db:migrate && node_modules/.bin/sequelize db:seed:all",
    "sequelize:all:undo": "node_modules/.bin/sequelize db:seed:undo:all && node_modules/.bin/sequelize db:migrate:undo:all",
    "sequelize:migrations": "node_modules/.bin/sequelize db:migrate",
    "sequelize:migrations:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "sequelize:migrations:undo:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "sequelize:seeds:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "sequelize:seeds": "node_modules/.bin/sequelize db:seed:all",
    "build": " tsc --build tsconfig.json",
    "start": "node dist/src/app.js --aaaMainProcess",
    "watch-ts": "tsc -w",
    "nodemon-dev": "nodemon dist/src/app.js",
    "dev": "concurrently \"npm run watch-ts\" \"npm run nodemon-dev\"",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register src/**/*.test.*s\"",
    "test-report": "nyc --reporter=lcov --reporter=text npm run test-dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@types/ajv-errors": "^2.0.1",
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-prettystream": "^0.1.32",
    "@types/co-body": "^6.1.0",
    "@types/config": "0.0.38",
    "@types/eslint": "^7.2.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa__cors": "^3.1.1",
    "@types/koa-bodyparser": "^4.3.6",
    "@types/koa-cors": "0.0.2",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-multer": "^1.0.1",
    "@types/koa-router": "^7.4.4",
    "@types/ps-node": "^0.1.0",
    "@types/uuid": "^8.3.4",
    "bunyan": "^1.8.14",
    "bunyan-prettystream": "^0.1.3",
    "config": "^3.3.3",
    "dotenv": "^16.0.0",
    "koa": "^2.13.1",
    "koa-404-handler": "^0.0.2",
    "koa-better-error-handler": "^7.0.1",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.1",
    "koa-logger": "^3.2.1",
    "koa-multer": "^1.0.2",
    "koa-router": "^10.1.1",
    "koa2-formidable": "^1.0.3",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "pg": "^8.7.3",
    "ps-node": "^0.1.6",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.1.3",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/koa": "^2.13.4",
    "@types/mocha": "^8.2.0",
    "@types/numeral": "^2.0.1",
    "@types/should": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "coffeescript": "^2.5.1",
    "concurrently": "^5.3.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^1.20.2",
    "faker": "^5.5.3",
    "istanbul": "^0.4.5",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "should": "^11.2.1",
    "sinon": "^6.1.3",
    "source-map-support": "^0.5.19",
    "supertest": "^3.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.2.1"
  }
